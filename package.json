{
  "name": "@cloudogu/ces-theme-tailwind",
  "license": "MIT",
  "private": false,
  "version": "0.7.1",
  "files": [
    "target",
    "tailwind.presets.cjs",
    "colormapping.cjs",
    "colormapping.json",
    "colorscheme.json",
    "ces-theme-tailwind-config.css"
  ],
  "main": "target/index.js",
  "module": "target/index.mjs",
  "types": "target/index.d.ts",
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider yarn storybook --ci",
    "build": "rm -rf target && tsup ./src/index.ts -d target --format esm,cjs --dts --sourcemap",
    "localbuild": "rm -rf localtarget && mkdir localtarget && yarn build && yarn pack --filename localtarget/archive.tgz && tar -xvzf localtarget/archive.tgz --strip-components 1 --directory localtarget",
    "tw": "npx tailwindcss build -o build/tailwind-generated.css",
    "storybook": "storybook dev -p 6006",
    "storybook-windows": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "storybook build",
    "test": "jest -w=20%",
    "test:watch": "jest -w=20% --watch",
    "prepublishOnly": "./scripts/pre-publish-checks.sh",
    "check": "./pre-publish-checks.sh",
    "fastPublish": "SKIP_LINT=true SKIP_TESTS=true SKIP_IMPORT_CHECKS=true yarn publish",
    "lint": "yarn eslint \"src/components/**\"",
    "fix": "yarn eslint --fix \"src/components/**\"",
    "circular": "dpdm --no-warning --no-tree --exit-code circular:1 --tsconfig tsconfig.json ./src/index.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
